{
  "buildCommand": null,
  "outputDirectory": "design-system",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/design-system/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
